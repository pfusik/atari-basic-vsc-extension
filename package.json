{
    "name": "atari-basic",
    "displayName": "Atari BASIC Syntax",
    "description": "Language support for Atari BASIC",
    "publisher": "MarcinJozwikowski",
    "repository": {
        "url": "https://github.com/marcin-jozwikowski/atari-basic-vsc-extension.git",
        "type": "git"
    },
    "icon": "AtariBasicSyntax.png",
    "version": "0.3.0",
    "engines": {
        "vscode": "^1.51.0"
    },
    "categories": [
        "Programming Languages"
    ],
    "contributes": {
        "languages": [
            {
                "id": "atari.basic",
                "aliases": [
                    "Atari Basic",
                    "Atari BASIC",
                    "basic",
                    "BASIC"
                ],
                "extensions": [
                    ".bas"
                ],
                "configuration": "./language-configuration.json"
            }
        ],
        "grammars": [
            {
                "language": "atari.basic",
                "scopeName": "source.atari.basic",
                "path": "./syntaxes/AtariBASIC.tmLanguage_4.json"
            }
        ]
    }
}
